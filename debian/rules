#!/usr/bin/make -f
# -*- makefile -*-
export DH_VERBOSE = 1
export DEB_BUILD_MAINT_OPTIONS = hardening=+all
export DEB_CFLAGS_MAINT_APPEND = -Wall -pedantic
export DEB_LDFLAGS_MAINT_APPEND = -W1,--as-needed

%:
	dh $@

override_dh_clean:
	dh_clean

override_dh_auto_build:
	dh_auto_build

override_dh_auto_install:
	dh_auto_install
